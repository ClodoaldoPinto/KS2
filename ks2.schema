query = '''insert into donor_yearly_fill (year, months) values($1, array[$2])'''
insert_plan = plpy.prepare(query, ['integer', 'integer'])

for i, d in enumerate(months[:-1]):
    year, month, batch = d['year'], d['month'], d['batch']
    if i > 1000: break
    rv = plpy.execute(dm_plan)
    dm = dict([(l['year'], l['months']) for l in rv])
    if year in dm:
        if month in dm[year]:
            continue
        plpy.execute(update_plan, [month, year])
    else:
        plpy.execute(insert_plan, [year, month])
    project_rank = 0
    team_rank = dict()
    for l in plpy.cursor(rsi_plan, (batch, months[i +1][2])):
        project_rank += 1
        team_rank[l['team']] = team_rank.get(l['team'], 0) + 1
        plpy.execute(update_donor_yearly_plan, [donor, year, points, team_rank[team], project_rank, month])

