        extract(day from current_timestamp)::integer as day,
        extract(hour from current_timestamp)::integer as hour;
"""
rv = plpy.execute(query)
day, hour = rv[0]['day'], rv[0]['hour']

if day != 1 or hour < 1 or hour > 3:
    return

query = """select update_donor_yearly($1, $2, $3, $4, $5, $6)"""
update_donor_yearly_plan = plpy.prepare(query, ['integer', 'smallint', 'real', 'integer', 'integer', 'smallint'])

query = """
    select distinct
        extract(year from um.data)::integer as year,
        extract(month from um.data)::integer as month,
        um.data_serial as batch
    from (
        select distinct d.data, d.data_serial
        from
            usuarios as u
            inner join
            datas as d on u.data = d.data_serial
        where
            u.data = (
                select data_serial
                from datas
                where date_trunc('day', data) = date_trunc('day', d.data)
                order by data desc limit 1
            )
            and
            date_trunc('month', d.data) < date_trunc('month', (select last_date from last_date) at time zone 'utc')
            and
            date_trunc('day', d.data) = (
                select date_trunc('day', data)
                from datas
                where date_trunc('month', data) = date_trunc('month', d.data)
                order by data desc limit 1
        )
        union
        select distinct d.data, d.data_serial
        from
            donor_work_old as dold
            inner join
            datas as d on d.data_serial = dold.data
    ) as um
    order by extract(year from um.data)::integer desc, extract(month from um.data)::integer desc
"""
months = plpy.execute(query)

query = """
    select year, months
    from donor_yearly_fill
"""
dm_plan = plpy.prepare(query)

query = """
    select
        d0.donor,
        d0.points - coalesce(d1.points, 0) as points,
        n_time as team
    from
        union_monthly_rank($1) as d0
        left outer join
        union_monthly_rank($2) as d1 on d0.donor = d1.donor
        inner join
        usuarios_indice as ui on ui.usuario_serial = d0.donor
    where d0.points - coalesce(d1.points, 0) > 0
    order by points desc
    ;
"""
rsi_plan = plpy.prepare(query, ['integer', 'integer'])
query = '''
    update donor_yearly_fill
    set months[array_upper(months, 1) +1] = $1
    where year = $2
'''
